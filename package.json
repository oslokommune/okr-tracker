{
  "name": "okr-tracker",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "test:unit": "NODE_ENV=test jest --testPathIgnorePatterns tests/firebase",
    "test:unit:firestore": "firebase emulators:exec --only firestore 'jest tests/firebase/firestore.test.js'",
    "dev": "firebase emulators:start --import=./mock_data & vite",
    "dev:test": "firebase emulators:start --import=./tests & vite",
    "build": "vite build --mode production",
    "build:ci": "vite build",
    "build:test": "vite build --mode staging",
    "deploy": ". ./.env.production && sed -i -e \"s|<host>|$VITE_API_GATEWAY_URL|g\" public/openapi.yaml && npm run build && sed -i -e \"s|$VITE_API_GATEWAY_URL|<host>|g\" public/openapi.yaml && firebase deploy --project=okr-prod-98cb",
    "deploy:test": "npm run build:test && firebase deploy --project=okr-dev-e2e3",
    "deploy:feature_branch": "npm run build && firebase hosting:channel:deploy --project=production $npm_config_name",
    "lint": "eslint ./src ./functions ./extras ./tests --ext .js,.vue,.gs",
    "lint:fix": "eslint ./src ./functions ./extras ./tests --ext .js,.vue,.gs --fix",
    "lint:style": "stylelint 'src/**/*.scss' 'src/**/*.vue'",
    "lint:style:fix": "stylelint 'src/**/*.scss' 'src/**/*.vue' --fix",
    "export_mock_data": "firebase emulators:export ./mock_data"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@oslokommune/punkt-vue2": "^9.2.6",
    "@vueuse/core": "^10.5.0",
    "d3": "^7.6.1",
    "d3-array": "^3.1.1",
    "d3-axis": "^3.0.0",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.0.1",
    "d3-transition": "^3.0.1",
    "date-fns": "^2.29.0",
    "dompurify": "^3.0.2",
    "firebase": "^9.23.0",
    "flatpickr": "^4.6.9",
    "focus-trap": "^7.0.0",
    "fuse.js": "^6.5.3",
    "html2canvas": "^1.4.1",
    "marked": "^4.3.0",
    "save-svg-as-png": "^1.4.17",
    "swagger-ui-dist": "^4.17.0",
    "tabbable": "^6.0.0",
    "tippy.js": "6.3.7",
    "v-click-outside": "^3.2.0",
    "v-tooltip": "^2.1.3",
    "vee-validate": "^3.4.14",
    "vue": "^2.7.14",
    "vue-flatpickr-component": "^8.1.7",
    "vue-i18n": "^8.26.8",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.3",
    "vue-select": "^3.20.2",
    "vue-toasted": "^1.1.28",
    "vuedraggable": "^2.24.3",
    "vuefire": "^2.2.5",
    "vuex": "^3.6.2",
    "vuexfire": "^3.2.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@firebase/rules-unit-testing": "^2.0.7",
    "@oslokommune/punkt-css": "^9.2.6",
    "@vitejs/plugin-vue2": "^2.2.0",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.15.14",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "firebase-tools": "^13.0.1",
    "jest": "^29.7.0",
    "postcss-html": "1.5.0",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
